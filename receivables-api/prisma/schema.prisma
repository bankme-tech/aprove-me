// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Payable {
  id           String   @id @default(uuid())
  value        Float
  emissionDate DateTime
  assignor     Int
}

model Assignor {
  id       Int      @id @default(autoincrement())
  document String
  email    String
  phone    String
  name     String
}