{
  "name": "nestjs-prisma",
  "version": "0.0.1",
  "description": "A NestJS project integrated with Prisma",
  "author": "Willer Santos",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
      "build": "nest build",
      "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
      "start": "nest start",
      "start:dev": "nest start --watch",
      "start:debug": "nest start --debug --watch",
      "start:prod": "node dist/main.js",
      "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
      "generate-schema": "cat src/*/*.prisma > prisma/base.prisma",
      "migrate": "npm run generate-schema && prisma migrate dev --schema=./prisma/base.prisma --name \"${npm_config_name}\" && npm run prisma:generate",
      "prisma:generate": "prisma-multischema",
      "prisma:migrate": "prisma migrate dev --name init",
      "prisma:studio": "prisma studio"
  },
  "dependencies": {
      "@nestjs/common": "^10.1.2",
      "@nestjs/config": "^3.2.2",
      "@nestjs/core": "^10.1.2",
      "@nestjs/platform-express": "^10.1.2",
      "@prisma/client": "^5.14.0",
      "body-parser": "^1.20.2",
      "class-transformer": "^0.5.1",
      "class-validator": "^0.14.0",
      "helmet": "^7.1.0",
      "prisma-multischema": "^1.1.4",
      "reflect-metadata": "^0.1.13",
      "rxjs": "^7.8.1"
  },
  "devDependencies": {
      "@nestjs/cli": "^10.1.10",
      "@nestjs/schematics": "^10.0.1",
      "@nestjs/testing": "^10.1.2",
      "@types/body-parser": "^1.19.5",
      "@types/express": "^4.17.17",
      "@types/node": "^18.15.11",
      "@types/passport-local": "^1.0.35",
      "@typescript-eslint/eslint-plugin": "^5.57.0",
      "@typescript-eslint/parser": "^5.57.0",
      "eslint": "^8.37.0",
      "eslint-config-prettier": "^8.8.0",
      "eslint-plugin-prettier": "^4.2.1",
      "prettier": "^2.8.7",
      "prisma": "^5.14.0",
      "supertest": "^6.3.3",
      "ts-node": "^10.9.1",
      "tsconfig-paths": "^4.2.0",
      "typescript": "^5.1.3"
  }
}
